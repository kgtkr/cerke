{
  "$schema": "https://raw.githubusercontent.com/rescript-lang/rescript-compiler/master/docs/docson/build-schema.json",
  "name": "cerke-frontend",
  "reason": {
    "react-jsx": 3
  },
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "bsc-flags": [
    "-bs-super-errors",
    "-bs-no-version-header"
  ],
  "package-specs": [
    {
      "module": "es6",
      "in-source": false
    }
  ],
  "namespace": true,
  "bs-dependencies": [
    "@rescript/react",
    "bs-webapi",
    "decco",
    "bs-bastet",
    "relude",
    "relude-random",
    "reason-future",
    "rescript-recoil"
  ],
  "ppx-flags": [
    "bs-let/ppx",
    "decco/ppx"
  ],
  "bs-dev-dependencies": [],
  "refmt": 3,
  "suffix": ".bs.js"
}
