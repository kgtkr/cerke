{
  "$schema": "https://raw.githubusercontent.com/rescript-lang/rescript-compiler/master/docs/docson/build-schema.json",
  "name": "cerke-ext",
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "bsc-flags": [
    "-bs-super-errors",
    "-bs-no-version-header"
  ],
  "package-specs": [
    {
      "module": "es6",
      "in-source": true,
      "public": [
        "ArrayExt",
        "GeneratorExt",
        "OptionExt",
        "TraversableExt"
      ]
    }
  ],
  "namespace": true,
  "bs-dependencies": [
    "bs-bastet",
    "relude",
    "relude-random"
  ],
  "ppx-flags": [],
  "bs-dev-dependencies": [],
  "suffix": ".js",
  "external-stdlib": "@rescript/std",
  "gentypeconfig": {
    "language": "typescript",
    "shims": {},
    "debug": {
      "all": false,
      "basic": false
    }
  }
}
