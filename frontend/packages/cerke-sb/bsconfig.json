{
  "$schema": "https://raw.githubusercontent.com/rescript-lang/rescript-compiler/master/docs/docson/build-schema.json",
  "name": "cerke-sb",
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "bsc-flags": [
    "-bs-super-errors",
    "-bs-no-version-header"
  ],
  "package-specs": [
    {
      "module": "es6",
      "in-source": true,
      "public": [
        "CerkeSb"
      ]
    }
  ],
  "namespace": false,
  "bs-dependencies": [],
  "ppx-flags": [],
  "bs-dev-dependencies": [],
  "suffix": ".bs.js"
}
