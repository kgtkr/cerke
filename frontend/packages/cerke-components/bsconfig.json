{
  "$schema": "https://raw.githubusercontent.com/rescript-lang/rescript-compiler/master/docs/docson/build-schema.json",
  "name": "cerke-components",
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "bsc-flags": [
    "-bs-super-errors",
    "-bs-no-version-header"
  ],
  "package-specs": [
    {
      "module": "es6",
      "in-source": true,
      "public": [
        "Ciurls",
        "Scoreboard",
        "WaterEntry",
        "Field",
        "ImageSprite"
      ]
    }
  ],
  "namespace": true,
  "bs-dependencies": [
    "@rescript/react",
    "bs-bastet",
    "relude",
    "relude-random",
    "reason-future",
    "cerke-entities"
  ],
  "ppx-flags": [
    "bs-let/ppx"
  ],
  "bs-dev-dependencies": [],
  "suffix": ".js",
  "external-stdlib": "@rescript/std",
  "gentypeconfig": {
    "language": "typescript",
    "shims": {},
    "debug": {
      "all": false,
      "basic": false
    }
  },
  "reason": {
    "react-jsx": 3
  }
}
